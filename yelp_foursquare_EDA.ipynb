   "source": [
    "url = 'https://api.foursquare.com/v3/places/search'\n",
    "\n",
    "headers = {\n",
    "    'accept': 'application/json',\n",
    "    'Authorization': <api-key>,
    "\n",
    "all_responses = []\n",
    "\n",
    "for i,j in zip(bike_stations['latitude'], bike_stations['longitude']):\n",
    "    params1 = {\"ll\": f\"{i},{j}\",\n",
    "    \"radius\": '1000'}\n",
    "    \n",
    "    response_test = requests.get(url, params = params1, headers = headers)\n",
    "    all_responses.append(response_test.json())\n",
    "\n",
    "all_responses2 = pd.json_normalize(all_responses)"
   ]
